<%=form_for @address do |f| %>

<div>
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>

<div>
  <%= f.label :address_line_one %>
  <%= f.text_field :address_line_one %>
</div>

<div>
  <%= f.label :address_line_two %>
  <%= f.text_field :address_line_two %>
</div>

<div>
  <%= f.label :city %>
  <%= f.text_field :city %>
</div>

<div>
  <%= f.label :state %>
  <%= f.text_field :state %>
</div>

<div>
 <%= f.label :zipcode %>
 <%= f.number_field :zipcode %>
</div>
<div>
  <%= f.label :phone_number %>
  <%= f.number_field :phone_number %>
</div>

  <%= f.submit "Update Address" %>
<% end %>
